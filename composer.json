{
  "name": "guillaumetissier/image-resizer",
  "type": "library",
  "description": "PHP implementation of an image resizer.",
  "homepage": "https://github.com/guillaumetissier/image-resizer",
  "keywords": ["jpeg", "gif", "png", "resize"],
  "license": "MIT",
  "version": "1.0.0",
  "authors": [{
    "name": "Guillaume Tissier",
    "email": "guillaume.tissier@yahoo.com"
  }],
  "autoload": {
    "psr-4": {
      "Guillaumetissier\\ImageResizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guillaumetissier\\ImageResizer\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-gd": "*",
    "guillaumetissier/path-utilities": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "friendsofphp/php-cs-fixer": "^3.92"
  },
  "scripts": {
    "cs": "php-cs-fixer fix",
    "test": "vendor/bin/phpunit tests"
  }
}
